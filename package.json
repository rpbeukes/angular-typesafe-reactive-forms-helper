{
  "name": "angular-typesafe-reactive-forms-helper",
  "version": "1.8.2",
  "description": "Use angular reactive forms with type-safety.",
  "main": "./bundles/angular-typesafe-reactive-forms-helper.umd.js",
  "scripts": {
    "build": "npm-run-all --sequential build:**",
    "build:remove-lib": "rimraf ./dist",
    "build:packagr": "npm run packagr",
    "test": "npm-run-all --sequential test:**",
    "test:dtslint": "dtslint --expectOnly ./tests/__typeTests__",
    "test:jest": "jest --config jestconfig.json",
    "format": "prettier --write 'src/**/*.ts' 'src/**/*.js'",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "perversion": "npm run lint",
    "version": "npm run format",
    "packagr": "ng-packagr --project ng-package.json",
    "package-version-published": "npm show ./ version",
    "package-json-version": "cat package.json | grep version | head -1 | awk -F: '{print $2}' | sed 's/[\",]//g' | sed 's/[ ]//g'",
    "update-latest-integration-tests": "npm-run-all --sequential update-latest-integration-tests:**",
    "update-latest-integration-tests-deprecated:ng6": "cp -R ./integrationTests/ng10/test-package-with-ng10/src/app/form/testform.component.spec.ts ./integrationTests/ng10/test-package-with-ng10/src/app/form/testform.component.ts ./integrationTests/ng10/test-package-with-ng10/src/app/form/testform.component.html ./integrationTests/ng6/test-package-with-ng6/src/app/form/",
    "update-latest-integration-tests:ng7": "cp -R ./integrationTests/ng10/test-package-with-ng10/src/app/form/testform.component.spec.ts ./integrationTests/ng10/test-package-with-ng10/src/app/form/testform.component.ts ./integrationTests/ng10/test-package-with-ng10/src/app/form/testform.component.html ./integrationTests/ng7/test-package-with-ng7/src/app/form/",
    "update-latest-integration-tests:ng8": "cp -R ./integrationTests/ng10/test-package-with-ng10/src/app/form/testform.component.spec.ts ./integrationTests/ng10/test-package-with-ng10/src/app/form/testform.component.ts ./integrationTests/ng10/test-package-with-ng10/src/app/form/testform.component.html ./integrationTests/ng8/test-package-with-ng8/src/app/form/",
    "update-latest-integration-tests:ng8:replace-ViewChild": "node ./buildScripts/replace-ng8-component-viewchild.js ./integrationTests/ng8/test-package-with-ng8/src/app/form/testform.component.ts",
    "update-latest-integration-tests:ng9": "cp -R ./integrationTests/ng10/test-package-with-ng10/src/app/form/testform.component.spec.ts ./integrationTests/ng10/test-package-with-ng10/src/app/form/testform.component.ts ./integrationTests/ng10/test-package-with-ng10/src/app/form/testform.component.html ./integrationTests/ng9/test-package-with-ng9/src/app/form/",
    "pack-tgz": "npm pack ./dist",
    "pack-to-dist-directory": "npm run pack-tgz && rimraf ./dist && mkdir ./dist && cp -R angular-typesafe-reactive-forms-helper*.tgz ./dist",
    "unpack-tgz": "rimraf ./package && tar -xzvf *.tgz",
    "remove-all-tgz": "rm -rf angular-typesafe-reactive-forms-helper-*.tgz",
    "install-tgz-to-all-integration-tests": "npm run remove-all-tgz && npm run pack-tgz && npm run integration-tests:copy-to-tests && npm-run-all --sequential integration-tests:install:**",
    "integration-tests": "npm run remove-all-tgz && npm run pack-tgz && npm-run-all --sequential integration-tests:**",
    "integration-tests:copy-to-tests": "cp -R angular-typesafe-reactive-forms-helper*.tgz ./integrationTests/angular-typesafe-reactive-forms-helper.tgz",
    "integration-tests-deprecated:install:ng6": "cd ./integrationTests/ng6/test-package-with-ng6 && npm i && npm i ../../angular-typesafe-reactive-forms-helper.tgz",
    "integration-tests-deprecated:build:ng6": "cd ./integrationTests/ng6/test-package-with-ng6 && npm run build",
    "integration-tests-deprecated:version:ng6": "cd ./integrationTests/ng6/test-package-with-ng6 && npm run ng -- --version",
    "integration-tests-deprecated:test-ci:ng6": "cd ./integrationTests/ng6/test-package-with-ng6 && export CI=true && npm run test -- --watch=false",
    "integration-tests:install:ng7": "cd ./integrationTests/ng7/test-package-with-ng7 && npm i && npm i ../../angular-typesafe-reactive-forms-helper.tgz",
    "integration-tests:list-installs:ng7": "cd ./integrationTests/ng7/test-package-with-ng7 && (npm ls --prod --depth 0 || exit 0) && (npm ls --dev --depth 0 || exit 0)",
    "integration-tests:build:ng7": "cd ./integrationTests/ng7/test-package-with-ng7 && npm run build",
    "integration-tests:version:ng7": "cd ./integrationTests/ng7/test-package-with-ng7 && npm run ng -- --version",
    "integration-tests:test-ci:ng7": "cd ./integrationTests/ng7/test-package-with-ng7 && export CI=true && npm run test -- --watch=false",
    "integration-tests:install:ng8": "cd ./integrationTests/ng8/test-package-with-ng8 && npm i && npm i ../../angular-typesafe-reactive-forms-helper.tgz",
    "integration-tests:list-installs:ng8": "cd ./integrationTests/ng8/test-package-with-ng8 && (npm ls --prod --depth 0 || exit 0) && (npm ls --dev --depth 0 || exit 0)",
    "integration-tests:build:ng8": "cd ./integrationTests/ng8/test-package-with-ng8 && npm run build",
    "integration-tests:version:ng8": "cd ./integrationTests/ng8/test-package-with-ng8 && npm run ng -- --version",
    "integration-tests:test-ci:ng8": "cd ./integrationTests/ng8/test-package-with-ng8 && export CI=true && npm run test -- --watch=false",
    "integration-tests:install:ng9": "cd ./integrationTests/ng9/test-package-with-ng9 && npm i && npm i ../../angular-typesafe-reactive-forms-helper.tgz",
    "integration-tests:list-installs:ng9": "cd ./integrationTests/ng9/test-package-with-ng9 && (npm ls --prod --depth 0 || exit 0) && (npm ls --dev --depth 0 || exit 0)",
    "integration-tests:build:ng9": "cd ./integrationTests/ng9/test-package-with-ng9 && npm run build",
    "integration-tests:version:ng9": "cd ./integrationTests/ng9/test-package-with-ng9 && npm run ng -- --version",
    "integration-tests:test-ci:ng9": "cd ./integrationTests/ng9/test-package-with-ng9 && export CI=true && npm run test -- --watch=false",
    "integration-tests:e2e:ng9:prod": "cd ./integrationTests/ng9/test-package-with-ng9 && export CI=true && npm run e2e -- --prod",
    "integration-tests:install:ng10": "cd ./integrationTests/ng10/test-package-with-ng10 && npm i && npm i ../../angular-typesafe-reactive-forms-helper.tgz",
    "integration-tests:list-installs:ng10": "cd ./integrationTests/ng10/test-package-with-ng10 && (npm ls --prod --depth 0 || exit 0) && (npm ls --dev --depth 0 || exit 0)",
    "integration-tests:build:ng10": "cd ./integrationTests/ng10/test-package-with-ng10 && npm run build",
    "integration-tests:version:ng10": "cd ./integrationTests/ng10/test-package-with-ng10 && npm run ng -- --version",
    "integration-tests:test-ci:ng10": "cd ./integrationTests/ng10/test-package-with-ng10 && export CI=true && npm run test -- --watch=false",
    "integration-tests:e2e-ci:ng10:prod": "cd ./integrationTests/ng10/test-package-with-ng10 && export CI=true && npm run e2e -- --prod",
    "start:ng6-deprecated": "cd ./integrationTests/ng6/test-package-with-ng6 && npm run build && npm run start -- --port 4206",
    "start:ng7": "cd ./integrationTests/ng7/test-package-with-ng7 && npm run start -- --port 4070",
    "start:ng7:prod": "cd ./integrationTests/ng7/test-package-with-ng7 && npm run start -- --prod --port 4071",
    "start:ng8": "cd ./integrationTests/ng8/test-package-with-ng8 && npm run start -- --port 4080",
    "start:ng8:prod": "cd ./integrationTests/ng8/test-package-with-ng8 && npm run start -- --prod --port 4081",
    "start:ng9": "cd ./integrationTests/ng9/test-package-with-ng9 && npm run start -- --port 4090",
    "start:ng9:prod": "cd ./integrationTests/ng9/test-package-with-ng9 && npm run start -- --prod --port 4091",
    "start:ng10": "cd ./integrationTests/ng10/test-package-with-ng10 && npm run start -- --port 4100",
    "start:ng10:prod": "cd ./integrationTests/ng10/test-package-with-ng10 && npm run start -- --prod --port 4101",
    "test-karma:ng6-deprecated": "cd ./integrationTests/ng6/test-package-with-ng6 && npm run test",
    "test-karma:ng7": "cd ./integrationTests/ng7/test-package-with-ng7 && npm run test",
    "test-karma:ng8": "cd ./integrationTests/ng8/test-package-with-ng8 && npm run test",
    "test-karma:ng9": "cd ./integrationTests/ng9/test-package-with-ng9 && npm run test",
    "test-karma:ng10": "cd ./integrationTests/ng10/test-package-with-ng10 && npm run test",
    "upgrade:ng6-deprecated": "cd ./integrationTests/ng6/test-package-with-ng6 && npm run ng -- update @angular/cli@^6 @angular/core@^6",
    "upgrade:ng7": "cd ./integrationTests/ng7/test-package-with-ng7 && npm run ng -- update @angular/cli@^7 @angular/core@^7",
    "upgrade:ng8": "cd ./integrationTests/ng8/test-package-with-ng8 && npm run ng -- update @angular/cli@^8 @angular/core@^8",
    "upgrade:ng9": "cd ./integrationTests/ng9/test-package-with-ng9 && npm run ng -- update @angular/cli@^9 @angular/core@^9",
    "upgrade:ng10": "cd ./integrationTests/ng10/test-package-with-ng10 && npm run ng -- update @angular/cli@^10 @angular/core@^10",
    "bump-ng-and-create-PR": "node ./scriptUtilities/bump-ng-and-create-PR.js",
    "is-ng-bump-required": "node ./scriptUtilities/is-ng-bump-required.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rpbeukes/angular-typesafe-reactive-forms-helper"
  },
  "keywords": [
    "angular",
    "reactive",
    "forms",
    "angular reactive forms",
    "typesafe"
  ],
  "author": "Ruan Beukes <ruan.coder@gmail.com>",
  "contributors": [
    "Matt <howiempt>"
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "~10.2.1",
    "@angular/compiler": "~10.2.1",
    "@angular/compiler-cli": "^10.2.1",
    "@angular/core": "~10.2.1",
    "@angular/forms": "~10.2.1",
    "@angular/platform-browser": "~10.2.1",
    "@types/jest": "^26.0.0",
    "@types/shelljs": "^0.8.7",
    "dtslint": "^4.0.0",
    "jest": "^26.0.1",
    "ng-packagr": "^10.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "shelljs": "^0.8.3",
    "ts-jest": "^26.1.0",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.0",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.2"
  }
}
