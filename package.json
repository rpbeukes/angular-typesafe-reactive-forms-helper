{
  "name": "angular-typesafe-reactive-forms-helper",
  "version": "1.5.0",
  "description": "Use angular reactive forms with type-safety.",
  "main": "lib/angularTypesafeReactiveFormsHelper.js",
  "types": "lib/angularTypesafeReactiveFormsHelper.d.ts",
  "scripts": {
    "build": "npm-run-all --sequential build:**",
    "build:remove-lib": "rimraf ./lib",
    "build:tsc": "tsc",
    "test": "npm-run-all --sequential test:**",
    "test:dtslint": "dtslint --expectOnly ./tests/__typeTests__",
    "test:jest": "jest --config jestconfig.json",
    "format": "prettier --write 'src/**/*.ts' 'src/**/*.js'",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "perversion": "npm run lint",
    "version": "npm run format",
    "package-version-published": "npm show ./ version",
    "package-json-version": "cat package.json | grep version | head -1 | awk -F: '{print $2}' | sed 's/[\",]//g' | sed 's/[ ]//g'",
    "update-latest-integration-tests": "npm-run-all --sequential update-latest-integration-tests:**",
    "update-latest-integration-tests:ng8": "cp -R ./integrationTests/ng9/test-package-with-ng9/src/app/form/testform.component.spec.ts ./integrationTests/ng9/test-package-with-ng9/src/app/form/testform.component.ts ./integrationTests/ng8/test-package-with-ng8/src/app/form/",
    "update-latest-integration-tests:ng7": "cp -R ./integrationTests/ng9/test-package-with-ng9/src/app/form/testform.component.spec.ts ./integrationTests/ng9/test-package-with-ng9/src/app/form/testform.component.ts ./integrationTests/ng7/test-package-with-ng7/src/app/form/",
    "update-latest-integration-tests:ng6": "cp -R ./integrationTests/ng9/test-package-with-ng9/src/app/form/testform.component.spec.ts ./integrationTests/ng9/test-package-with-ng9/src/app/form/testform.component.ts ./integrationTests/ng6/test-package-with-ng6/src/app/form/",
    "pack-tgz": "npm pack",
    "unpack-tgz": "rimraf ./package && tar -xzvf *.tgz",
    "remove-all-tgz": "rm -rf angular-typesafe-reactive-forms-helper-*.tgz",
    "install-tgz-to-all-integration-tests": "npm run remove-all-tgz && npm run pack-tgz && npm run integration-tests:copy-to-tests && npm-run-all --sequential integration-tests:install:**",
    "integration-tests": "npm run remove-all-tgz && npm run pack-tgz && npm-run-all --sequential integration-tests:**",
    "integration-tests:copy-to-tests": "cp -R angular-typesafe-reactive-forms-helper*.tgz ./integrationTests/angular-typesafe-reactive-forms-helper.tgz",
    "integration-tests:install:ng6": "cd ./integrationTests/ng6/test-package-with-ng6 && npm i && npm i ../../angular-typesafe-reactive-forms-helper.tgz",
    "integration-tests:test-ci:ng6": "cd ./integrationTests/ng6/test-package-with-ng6 && cross-env CI=true ng test --watch=false",
    "integration-tests:install:ng7": "cd ./integrationTests/ng7/test-package-with-ng7 && npm i && npm i ../../angular-typesafe-reactive-forms-helper.tgz",
    "integration-tests:test-ci:ng7": "cd ./integrationTests/ng7/test-package-with-ng7 && cross-env CI=true ng test --watch=false",
    "integration-tests:install:ng8": "cd ./integrationTests/ng8/test-package-with-ng8 && npm i && npm i ../../angular-typesafe-reactive-forms-helper.tgz",
    "integration-tests:test-ci:ng8": "cd ./integrationTests/ng8/test-package-with-ng8 && cross-env CI=true ng test --watch=false",
    "integration-tests:install:ng9": "cd ./integrationTests/ng9/test-package-with-ng9 && npm i && npm i ../../angular-typesafe-reactive-forms-helper.tgz",
    "integration-tests:test-ci:ng9": "cd ./integrationTests/ng9/test-package-with-ng9 && cross-env CI=true ng test --watch=false"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rpbeukes/angular-typesafe-reactive-forms-helper"
  },
  "keywords": [
    "angular",
    "reactive",
    "forms",
    "angular reactive forms",
    "typesafe"
  ],
  "author": "Ruan Beukes <ruan.coder@gmail.com>",
  "contributors": [
    "Matt <howiempt>"
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "^9.0.6",
    "@angular/core": "^9.0.6",
    "@angular/forms": "^9.0.6",
    "@angular/platform-browser": "^9.0.6",
    "@types/jest": "^25.2.1",
    "@types/shelljs": "^0.8.7",
    "conditional-type-checks": "^1.0.5",
    "cross-env": "^7.0.2",
    "dtslint": "^3.4.1",
    "jest": "^25.2.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "shelljs": "^0.8.3",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2",
    "tslib": "^1.11.1",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "files": [
    "lib/**/*"
  ]
}
